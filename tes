const updateUser = async (req, res) => {
  try {

    const users = await User.findByIdAndUpdate(req.params.id, req.body, {
      new: true,
      runValidator: true
    })

    res.status(200).json({
      status: true,
      data: users,
      method: req.method,
      url: req.url,
      message: 'Data berhasil Di Update'

    })
  } catch (err) {
    res.status(200).json({
      status: false,
      message: 'Data Gagal Di Update'
    })
  }
};